{
  "agent": {
    "name": "ReAct-Agent",
    "version": "2.0.0",
    "max_iterations": 5,
    "timeout": 60,
    "verbose": true,
    "auto_check": true
  },
  "model": {
    "provider": "ollama",
    "name": "qwen2.5:7b",
    "base_url": "http://localhost:11434",
    "temperature": 0.7,
    "max_tokens": 4096
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/agent.log"
  }
}