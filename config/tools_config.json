{
  "tools": {
    "shell": {
      "enabled": true,
      "timeout": 30,
      "safe_commands": [
        "ls", "pwd", "whoami", "date", "cal", "uptime",
        "cat", "head", "tail", "wc", "grep", "find",
        "echo", "printf", "which", "whereis", "type",
        "df", "du", "free", "top", "ps", "uname",
        "id", "groups", "hostname", "env", "printenv",
        "file", "stat", "md5sum", "sha256sum",
        "tree", "realpath", "dirname", "basename",
        "sort", "uniq", "cut", "tr", "awk", "sed",
        "diff", "comm", "join", "paste",
        "tar", "gzip", "gunzip", "zip", "unzip",
        "curl", "wget", "ping", "ifconfig", "ip",
        "git", "python", "python3", "pip", "node", "npm"
      ],
      "dangerous_commands": [
        "rm -rf /", "rm -rf /*", "mkfs", "dd if=/dev/zero",
        ":(){ :|:& };:", "chmod -R 777 /", "chown -R",
        "shutdown", "reboot", "halt", "init 0", "init 6",
        "kill -9 -1", "pkill -9", "> /dev/sda",
        "mv /* /dev/null", "wget | sh", "curl | bash"
      ]
    },
    "file": {
      "enabled": true,
      "max_file_size": 10485760,
      "allowed_extensions": [
        ".py", ".js", ".ts", ".json", ".yaml", ".yml",
        ".md", ".txt", ".html", ".css", ".xml",
        ".c", ".cpp", ".h", ".hpp", ".java", ".go", ".rs",
        ".sh", ".bash", ".zsh", ".fish",
        ".sql", ".csv", ".log", ".conf", ".ini"
      ],
      "protected_paths": [
        "/etc", "/usr", "/bin", "/sbin", "/boot",
        "/root", "/sys", "/proc", "/dev"
      ]
    },
    "network": {
      "enabled": true,
      "timeout": 30,
      "max_download_size": 104857600,
      "allowed_protocols": ["http", "https"],
      "search_engines": {
        "duckduckgo": "https://duckduckgo.com/html/?q=",
        "google": "https://www.google.com/search?q=",
        "baidu": "https://www.baidu.com/s?wd="
      }
    },
    "data": {
      "enabled": true,
      "max_rows": 100000
    },
    "image": {
      "enabled": true,
      "max_size": 52428800,
      "supported_formats": ["JPEG", "PNG", "GIF", "BMP", "WEBP"]
    }
  }
}
